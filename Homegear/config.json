{
  "name": "Homegear",
  "version": "0.2.13",
  "slug": "rpi-homegear",
  "description": "Homegear as a Hassio add-on",
  "startup": "before",
  "boot": "auto",
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "arch": [
    "armhf"
  ],
  "map": ["config:rw", "share:rw"],
  "host_network": true,
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
     "/dev/ttyUSB0:/dev/ttyUSB0:rwm",
     "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
     "/dev/ttyACM0:/dev/ttyACM0:rwm"
  ]
}
